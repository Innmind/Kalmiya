{
    "name": "innmind/kalmiya",
    "type": "library",
    "description": "Personal assistant",
    "keywords": ["assistant"],
    "homepage": "http://github.com/Innmind/Kalmiya",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Kalmiya/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/cli": "~2.0",
        "music-companion/apple-music": "~2.0",
        "innmind/silent-cartographer": "^2.0",
        "innmind/cli-framework": "^1.2",
        "innmind/http-framework": "^2.2",
        "innmind/templating": "^2.0",
        "innmind/ipc": "^3.0",
        "innmind/genome": "^3.0",
        "innmind/infrastructure": "^1.1",
        "innmind/dependency-graph": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Kalmiya\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Kalmiya\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "vimeo/psalm": "^3.7 <3.11"
    },
    "bin": ["kalmiya"]
}
